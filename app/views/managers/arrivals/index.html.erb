	<div class="container">
		<div class="row">
			<table class="col-l-6 table table-bordered">
			<h3>入荷履歴</h3>
			<p>◯ 件の履歴があります</p>
			<p>◯◯◯◯年◯月◯日</p>
		    <thead>
		        <tr>
		            <th>入荷日</th>
		            <th>商品名</th>
		            <th>価格（税別）</th>
		            <th>個数</th>
		     		<th>小計</th>
		        </tr>
		    </thead>
		    <% @contacts.each do |contact|%>
			    <tbody>
			        <tr>
			            <th><%= contact.title %></th>
			            <td><%= contact.title %></td>
			            <td><%= contact.created_at %></td>
			            <td><%= contact.created_at %></td>
			        </tr>
			    </tbody>
		    <% end %>
			</table>
		</div>
	</div>






<div class="container">
	<div class="row">
	 <div class="col-lg-12">
	 	<%= form_for(@contact) do |f| %>
			<div class="row">
			 <div class="col-lg-6">メールアドレス</div><div class="col-lg-6"><%= f.text_field :email %></div>
			</div>
			<div class="row">
			 <div class="col-lg-6">件名</div><div class="col-lg-6"><%= f.text_field :title %></div>
			</div>
			<div class="row">
	 		 <div class="col-lg-6">お問い合わせ内容</div><div class="col-lg-6"><%= f.text_field :body %></div>
		 	</div>
		 	<div class="row">
	 		 <div class="col-lg-12"><%= f.submit '送信' %></div>
		 	</div>
	 	<% end %>
	 </div>
	</div>
</div>
